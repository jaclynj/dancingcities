<div class = "admins">
<h1>Admin Log In</h1>
<div class = "messages admin-small-box">
  <%= form_tag admin_sessions_path do %>
    <%=label_tag :admin_name %>
    <div class = "form"><%=text_field_tag :admin_name, params[:admin_name] %></div><br><br>
    <%=label_tag :password %>
    <div class = "form"><%=password_field_tag :password %><div></br>
    <%=submit_tag nil, :class => "splash-button submit"%>
<% end %>
</div>
</div>

